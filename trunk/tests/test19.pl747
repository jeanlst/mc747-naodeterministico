/* Teste de comando repetitivo */

int main() {
	int a, b, c;
     while (a<c)
     	a = a + c;

     do {
     	b = c;
	a = b;
	c = a;
     } while (1);  // erro semantico

     for i = 0 to b do {  // erro semantico
     	a = c;
	c = b;
 	b = a;
     }

     for i = b downto 0 do {  // erro semantico
     	a = c;
	c = b;
 	b = a;
     }
     return 0;
}